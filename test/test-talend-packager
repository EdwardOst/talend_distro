set -e
set -x

script_path=$(readlink -e "${BASH_SOURCE[0]}")
script_dir="${script_path%/*}"

source ${script_dir}/lib/packager/talend-packager.sh


rm -f myapp.tgz
rm -f t0_docker_create_customer-0.1.0-SNAPSHOT.zip
rm -rf myapp
rm -rf  t0_docker_create_customer-0.1.0-SNAPSHOT

talend_packager -m "${script_dir}/job_manifest.cfg" -g "com/talend/se/packager" -a "myapp"
